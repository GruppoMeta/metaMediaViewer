      "dist/metaMediaViewer.css"
  ],
  "dependencies": {
    "jquery": "^1.8.0",
    "angular": "^1.4.0",
    "bootstrap": "^3.3.5",
    "angular-bootstrap": "^1.2.5",
    "components-font-awesome":"^4.5.0",
    "angular-pdfjs-viewer":"^0.6.1",
    "videogular": "~1.2.5",
    "videogular-buffering": "~1.2.5",
    "videogular-controls": "~1.2.5",
    "videogular-overlay-play": "~1.2.5",
    "videogular-poster": "~1.2.5",
    "videogular-themes-default": "~1.2.5"
  },
  "keywords": [
      "multimedia",
      "viewer",
      "carousel",
      "image",
      "video",
      "audio",
      "pdf"
